<h3 class="page-header"><a data-toggle="collapse" data-target="#notification">お知らせ一覧</a></h3>

<div id="notification" class="collapse">
  <% @notifications.each do |notification| %>
    <p class="notification-display"><%= notification.content %>
    <%= notification.created_at.strftime("%Y/%m/%d") %></p>
  <% end %>
</div>
